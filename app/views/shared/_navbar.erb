<lux-library-header app-name="Bibdata" abbr-name="Bibdata" theme="dark">
      <lux-menu-bar type="main-menu" :menu-items="[
        {name: 'Home', component: 'Home', href: '/'},
        {name: 'Events', component: 'Events', href: '<%= main_app.events_path %>'},
        {name: 'Delivery Locations', component: 'Delivery', href: '<%= main_app.delivery_locations_path %>'},
        {name: 'Holding Locations', component: 'Holding', href: '<%= main_app.holding_locations_path %>'},
        {name: 'Libraries', component: 'Libraries', href: '<%= main_app.libraries_path %>'},
        <% if current_user %>
          {name: 'Logout', component: 'Libraries', href: '<%= main_app.destroy_user_session_path %>', method: 'DELETE'}
        <% else %>
          {name: 'Login', component: 'Libraries', href: '<%= main_app.user_cas_omniauth_authorize_path %>', method: 'POST'}
        <% end %>
]" />
</lux-library-header>
